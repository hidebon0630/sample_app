<% posts.each do |post| %>
  <p>いいね件数: <%= post.likes.count %></p>
  <% if current_user.already_liked?(post) %>
    <%= button_to 'いいねを取り消す', post_like_path(post), method: :delete %>
  <% else %>
    <%= button_to post_likes_path(post) do %>
      <i class="fas fa-heart"></i>
    <% end %>
  <% end %>
  <p>いいねしたユーザー</p>
  <% post.liked_users.each do |user| %>
    <li><%= user.email %></li>
  <% end %>
<% end %>
